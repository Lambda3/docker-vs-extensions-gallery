<service>
  <id>pgc</id>
  <name>PrivateGalleryCreator</name>
  <description>Creates the vs gallery feed.</description>
  <executable>c:\\pgc\\PrivateGalleryCreator.exe</executable>
  <arguments>-w --name="%PGC_GALLERY_NAME%" --output=C:\\inetpub\\wwwroot\\feed.xml --input=%PGC_VSIX_LOCATION%</arguments>
  <logmode>rotate</logmode>
  <startmode>Automatic</startmode>
</service>
